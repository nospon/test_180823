<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.domain.mapper.TicketVO">

	<select id="ticketread" resultType="aaa.bbb.ccc.VO.TicketVO">
		select a.age_num, b.id from ticket a , member b where a.engname = b.engname ;
	</select>
	
	<select id="read" resultType="aaa.bbb.ccc.VO.TicketVO">
		select * from ticket;
	</select>
</mapper>